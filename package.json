{
  "name": "GCashMall",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "cordova": {
    "platforms": [
      "android"
    ],
    "plugins": {}
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "start": "netlify dev",
    "postinstall": "node scripts/cordova-android-jdk-fix.js",
    "cordova:prepare": "npm run build && npx cordova prepare android",
    "cordova:build": "npm run build && npx cordova build android",
    "cordova:build:release": "npm run build && npx cordova build android --release",
    "cordova:run": "npm run build && npx cordova run android",
    "cordova:emulate": "npm run build && npx cordova emulate android"
  },
  "dependencies": {
    "bcryptjs": "^3.0.3",
    "cloudinary": "^2.8.0",
    "jsonwebtoken": "^9.0.3",
    "mongodb": "^6.0.0",
    "nodemailer": "^7.0.12",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-router-dom": "^7.10.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.5",
    "@types/react-dom": "^19.2.3",
    "@types/youtube": "^0.1.2",
    "@vitejs/plugin-react": "^5.1.1",
    "cordova": "^13.0.0",
    "cordova-android": "^12.0.1",
    "eslint": "^9.39.1",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "globals": "^16.5.0",
    "netlify-cli": "^23.12.2",
    "sharp": "^0.34.5",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.46.4",
    "vite": "^7.2.4"
  }
}
